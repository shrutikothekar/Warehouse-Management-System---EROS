@model IEnumerable<eros.Models.CheckWarrantyStatus>

<div class="table-responsive">
    <table class="table table-bordered table-hover text-center table-sm mt-0 pt-0" id="dataTable" width="100%" cellspacing="0">
        <thead class="thead-light">
            <tr>
                <th>@Html.DisplayNameFor(model => model.productcode)</th>
                <th>@Html.DisplayNameFor(model => model.productname)</th>
                <th>@Html.DisplayNameFor(model => model.warranty)</th>
                <th>@Html.DisplayNameFor(Model => Model.status)</th> 
            </tr>
        </thead>
        <tbody>
           @if (Model.Count() > 0)
            {
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.DisplayFor(modelItem => item.productcode)</td>
                        <td>@Html.DisplayFor(modelItem => item.productname)</td>
                        <td>@Html.DisplayFor(modelItem => item.warranty)</td>
                        <td>@Html.DisplayFor(modelItem => item.status)</td>
                    </tr>
                }
            }
            
        </tbody>
    </table>
</div>


